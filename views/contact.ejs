<div class="container mt-3">
  <row>
    <col-md-8>
      <h2>Daftar Kontak</h2>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Nama</th>
            <th scope="col">Nomor HP</th>
            <th scope="col">Aksi</th>
          </tr>
        </thead>
        <tbody>
          <% contacts.forEach((contact, index) => { %>
            <tr>
              <th scope="row"><%= index + 1 %></th>
              <td><%= contact.nama %></td>
              <td><%= contact.nomorHp %></td>
              <td><a href="" class="btn btn-success badge rounded-pill"><i class="bi bi-info-circle"></i> detail</a></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </col-md-8>
  </row>
</div>